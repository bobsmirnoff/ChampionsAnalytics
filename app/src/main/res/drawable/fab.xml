<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@android:color/secondary_text_light" />
        </shape>
    </item>
    <item android:bottom="2dp">
        <shape android:shape="oval">
            <size
                android:width="70dp"
